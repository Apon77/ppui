# Healthd
genfscon sysfs /devices/soc/soc:qcom,bcl/power_supply      u:object_r:sysfs_batteryinfo:s0
genfscon sysfs /devices/soc/msm-bcl-20/power_supply        u:object_r:sysfs_batteryinfo:s0

# Rmt
genfscon debugfs /rmt_storage	u:object_r:debugfs_rmt:s0

# RTC
genfscon sysfs /devices/soc/qpnp-rtc-8/rtc                  u:object_r:sysfs_rtc:s0

# Touchscreen
genfscon proc /touchpanel	u:object_r:proc_touchpanel:s0

# Earpiece gain
genfscon sysfs /sys/kernel/sound_control/earpiece_gain                      u:object_r:sysfs_egain:s0

# Headphone gain
genfscon sysfs /sys/kernel/sound_control/headphone_gain                     u:object_r:sysfs_headgain:s0

# Microphone gain
genfscon sysfs /sys/kernel/sound_control/mic_gain                           u:object_r:sysfs_micgain:s0

# Speaker gain
genfscon sysfs /sys/kernel/sound_control/speaker_gain                       u:object_r:sysfs_sgain:s0

# USB force fastcharge
genfscon sysfs /sys/kernel/fast_charge/force_fast_charge                                     u:object_r:sysfs_fcharge:s0
